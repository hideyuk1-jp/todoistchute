{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "version": "2.1.0",
  "default_locale": "en",
  "browser_action": {
    "default_icon": "img/icon.png"
  },
  "icons": {
    "128": "img/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.todoist.com/*"],
      "js": ["js/lib/jquery-3.6.4.min.js", "js/tc.js", "js/tc_calender.js"],
      "css": ["css/tc.css"],
      "run_at": "document_end"
    }
  ],
  "options_page": "options.html",
  "permissions": ["storage"]
}
